{
  "name": "coopeibm-dataapi",
  "version": "1.3.7",
  "description": "Apis para Cooperativa IBM",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^16.14.2",
    "npm": "^8.6.0"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t coopeibm-dataapi .",
    "docker:run": "docker run -p 3000:3000 -d coopeibm-dataapi",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "xxprestartxx": "npm run build",
    "xxstartxx": "node -r source-map-support/register .",
    "start": "node dist/.",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git"
  },
  "author": "sergio.ramos@ibm.com",
  "license": "SEE LICENSE IN LICENSE file",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^4.1.2",
    "@loopback/core": "^3.1.2",
    "@loopback/extension-health": "^0.4.13",
    "@loopback/repository": "^4.1.2",
    "@loopback/rest": "^11.1.2",
    "@loopback/rest-crud": "^0.13.2",
    "@loopback/rest-explorer": "^4.1.2",
    "@loopback/service-proxy": "^4.1.2",
    "cfenv": "^1.2.4",
    "ibm_db": "^2.8.1",
    "loopback-connector-cloudant": "^2.5.0",
    "loopback-connector-db2": "^2.2.3",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@loopback/build": "^8.1.2",
    "@loopback/eslint-config": "^12.0.4",
    "@loopback/testlab": "^4.1.2",
    "@types/node": "^17.0.24",
    "eslint": "^8.13.0",
    "shelljs": "^0.8.5",
    "source-map-support": "^0.5.21",
    "tsc-watch": "^5.0.3",
    "typescript": "~4.6.3"
  },
  "resolutions": {
    "webpack": "^5.72.0"
  }
}
